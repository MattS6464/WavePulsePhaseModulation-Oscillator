, 			{
				"box" : 				{
					"code" : "/* ===========================/*\r\n\timplementation of PPM\r\n/* ===========================*/\r\n\r\n//smoother\r\nsmooth(x, time) {\r\n\tHistory prev;\r\n\tprev = prev + time * (x - prev);\r\n\t\r\n\treturn prev;\r\n}\r\n\r\n//window\r\nwindow(t, n, x) {\r\n\tif(t==0){//sin window\r\n\t\treturn sin(PI*x);\r\n\t}\r\n\tif(t==1) {//ramp window\r\n\t\treturn 1-x;\r\n\t}\r\n\tif(t==2) {//square window\r\n\t\treturn 1;\r\n\t}\r\n\tif(t==3) {//Gauss Window\r\n\t\treturn exp(-1*pow((x-0.5)/((n*0.2)+0.05),2));\r\n\t}\r\n\tif(t==4) {//Tukey Window\r\n\t\tif(x > 0.5){\r\n\t\t\tx = 1-x;\r\n\t\t}\r\n\t\tif(x < n/2) {\r\n\t\t\treturn 0.5*(1-cos((TWOPI*x) / n));\r\n\t\t\t\r\n\t\t}else {\r\n\t\t\treturn 1;\r\n\t\t}\r\n\t}\r\n}\r\n\r\n//parameter inputs, using Ra and Rb instead of R_1 and R_2 ect\r\nHistory phase(0);\r\n\r\nd = smooth(in2, 0.05);\r\nv = smooth(in3, 0.05);\r\n\r\nRa = smooth(in4, 0.05);\r\nRb = smooth(in5, 0.05);\r\n\r\nPa = smooth(in6, 0.05);\r\nPb = smooth(in7, 0.05);\r\n\r\nIa = smooth(in8, 0.05);\r\nIb = smooth(in9, 0.05);\r\n\r\nWs = smooth(in10, 0.05);\t//window shape\r\nWt = in11;\t\t\t\t//window type\r\n\r\nincrement = mtof(in1) / samplerate;\r\n\r\n//PPM equation\r\nif (phase < d) {\r\n\tout1 = v*phase/d\r\n\t\t+ Ia*window(4,0.3,d*0.5)\t\t\r\n\t\t\t* window(Wt,Ws,phase/d) * \r\n\t\t\t\tsin(TWOPI*(phase*Ra/d+Pa));\r\n\t\r\n} else {\t\r\n\tout1 = (1-v)*(phase-d)/(1-d)+v \r\n\t\t+ Ib*window(4,0.3,(1-d)*0.5)\r\n\t\t\t* window(Wt,Ws,(phase-d)/(1-d)) * \r\n\t\t\t\tsin(TWOPI*((Rb*(phase-d)/(1-d))+Pb));\r\n}\r\n\r\n\r\nphase += increment;\nwhile (phase > 1.0) phase -= 1.0;",
